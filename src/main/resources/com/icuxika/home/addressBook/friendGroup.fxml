<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.icuxika.controller.home.addressBook.FriendGroupController">
    <children>
        <HBox alignment="CENTER">
            <children>
                <Button fx:id="addButton" mnemonicParsing="false" text="添加测试"/>
                <Button fx:id="updateButton" mnemonicParsing="false" text="更新测试"/>
            </children>
        </HBox>
        <TreeView fx:id="friendTreeView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
    </children>
</VBox>
